<Window x:Class="ChmlFrpLauncher_cs.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="ChmlFrpLauncher" Height="450" Icon="pack://application:,,,/icon/favicon.ico" Width="800">
    
    <Grid >
        
        <Border BorderThickness="1">
            <ContentControl >
                <ContentControl.Content>
                    <Frame Source="pack://application:,,,/pages/Launcher.xaml" Margin="0,90,400,0" />
                </ContentControl.Content>
            </ContentControl>
        </Border>
        
        <Border BorderThickness="1" >
            <ContentControl >
                <ContentControl.Content>
                    <Frame Source="pack://application:,,,/pages/Textbox-ini.xaml" Margin="400,90,0,0" />
                </ContentControl.Content>
            </ContentControl>
        </Border>
        
    </Grid>
</Window>
