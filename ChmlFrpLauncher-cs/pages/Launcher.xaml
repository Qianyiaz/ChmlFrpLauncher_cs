<Page x:Class="ChmlFrpLauncher_cs.pages.Launcher"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ChmlFrpLauncher_cs.pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Launcher">

<StackPanel>
            <Button BorderBrush="Gray" Foreground="Black"  Background="#EEEEEE" Content="启动 frpc"  Click="Launch" FontSize="13"  HorizontalAlignment="Center" Height="49" Width="169" Margin="0,75,0,0">
                <Button.Resources>
                    <Style TargetType="{x:Type Border}">
                        <Setter Property="CornerRadius" Value="20"/>
                    </Style>
                </Button.Resources>
            </Button>
</StackPanel>

</Page>
