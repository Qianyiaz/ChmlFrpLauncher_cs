<Page x:Class="ChmlFrpLauncher_cs.Pages.ChmlFrp.HomePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ChmlFrpLauncher_cs.Pages.ChmlFrp"
      mc:Ignorable="d" 
      d:DesignHeight="370"
      d:DesignWidth="620">

    <Grid>
        <StackPanel HorizontalAlignment="Right" VerticalAlignment="Top">
            <Ellipse Width="100" Height="100" >
                <Ellipse.Fill>
                    <ImageBrush x:Name="UserImage" ImageSource="\Assets\Icons\SolarUserCircleLineDuotone.png" Stretch="UniformToFill"/>
                </Ellipse.Fill>
            </Ellipse>
        </StackPanel>
    </Grid>
</Page>
